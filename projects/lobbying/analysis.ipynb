   "metadata": {},
   "outputs": [],
   "source": [
    "companies_df = (\n",
    "    df[\n",
    "        [\n",
    "            \"client_name_on_filing_document\",\n",
    "            \"client_id\",\n",
    "            \"organization\",\n",
