    "color_dict = {\n",
    "    \"oil and gas\": \"dimgrey\",\n",
    "    \"investor owned utilities\": \"dimgrey\",\n",
    "    \"business advocacy\": \"cornflowerblue\",\n",
    "    \"independent power producers\": \"dimgrey\",\n",
